name: "Garry's Mod Lua"
comment: "Garry's Mod Lua"
scopeName: "source.lua"

fileTypes: [ "lua" ]
firstLineMatch: """
(?x)

	# Change syntax to Lua if we find an interpreter directive
	\\A \\#! .*? \\b lua \\b

"""

patterns: [

	## Garry's Mod Specifics ###################################################

	{	# C-style single-line comments
		name: "comment.line.double-slash.lua"

		begin: "//"
		end: "\\n"
	}

	{	# C-style block comments
		name: "comment.block.lua"

		begin: "/\\*"
		end: "\\*/"
	}

	{	# continue keyword
		name: "keyword.control.lua"

		match: "\\bcontinue\\b"
	}

	{	# C-style logical operator
		name: "keyword.operator.lua"

		match: "&&|\\|\\||!"
	}

	## Functions ###############################################################

	{	# Function definition
		name: "meta.function.lua"

		begin: """
(?x)

			# Match the function keyword
			\\b ( function )

			# Optional: function name
			(?:
				# Scope
				\\s+ ( (?: [a-zA-Z_]\\w* [.:] )* )
				# Function name
				( [a-zA-Z_]\\w* ) \\s*
			)?

			# Match the opening parameters brace
			\\(
"""
		beginCaptures:
			1: name: "keyword.control.lua"
			2: name: "entity.name.function.scope.lua"
			3: name: "entity.name.function.lua"

		end: "\\)"

		# Match all parameters inside the brackets, comma delimited
		patterns: [
			name: "variable.parameter.function.lua"
			match: "([^,])"
		]
	}

	## Constants ###############################################################

	{	# Numbers
		name: "constant.numeric.lua"

		match: """
(?x)

			\\b (
				# Hex numbers
				0x [\\d a-f A-F]+
				# Optional decimal
				( \\. [\\d a-f A-F]+ )?
			|
				# Integers with optional exponent
				\\d+
				( [eE] -? \\d+ )?
			|
				# Decimals with optional exponent
				\\d* \\. \\d+
				( [eE] -? \\d+ )?
			)
		
"""
	}

	{	# Values
		name: "constant.language.lua"

		match: """
(?x)

			\\b (
				nil | true | false | _G
				| _VERSION | math\\.pi | math\\.huge
			) \\b
		|
			\\.{3}
		
"""
	}

	## Comments ################################################################

	{	# Multi-line comments
		name: "comment.block.lua"

		begin: "--\\[(=*)\\["
		end: "\\]\\1\\]"
	}

	{	# Single-line comments
		name: "comment.line.double-dash.lua"

		begin: "--"
		end: "\\n"
	}

	## Strings #################################################################

	{	# Single quotes
		name: "string.quoted.single.lua"

		begin: "'"
		end: "'"

		patterns: [
			include: "#escaped_chars"
		]
	}

	{	# Double quotes
		name: "string.quotes.double.lua"

		begin: "\""
		end: "\""

		patterns: [
			include: "#escaped_chars"
		]
	}

	{	# Multiline string
		name: "string.quotes.other.multiline.lua"

		begin: """
(?x)

			# Match the starting double brace
			\\[ ( =* ) \\[
		
"""
		end: "\\]\\1\\]"
	}

	## Keywords ################################################################

	{	# Explicit Keywords
		name: "keyword.control.lua"

		match: """
(?x)

			\\b (
				break | do | else | for
				| if | elseif | return | then
				| repeat | while | until | end
				| function | local | in | goto
			) \\b
		
"""
	}

	{	# Operators
		name: "keyword.operator.lua"

		match: """
(?x)

			# Mathematical Operators
			\\+ | - | \\* | \\/ | \\^ | %
			# Auxialiary Operators
			| \\#
			# Logical Operators
			| ==? | ~= | <=? | >=?
			# Boolean Operators
			| \\b ( and | or | not ) \\b
			# Concatenation Operator
			| (?<! \\. ) \\.{2} (?! \\. )
		
"""
	}

	{	# Self keyword
		name: "variable.language.self.lua"

		match: """
(?x)

			# Check we're not scoped
			(?<! [^.] . | : )
			# Match the word "self"
			\\b self \\b
		
"""
	}

	## Library functions #######################################################

	{	# Global functions
		name: "support.function.library.lua"

		match: """
(?x)

			# Check we are *not* scoped
			(?<! [^.] . | : )
			# Check for built-in functions
			\\b (
				assert | collectgarbage | dofile | error
				| getfenv | getmetatable | ipairs | loadfile
				| loadstring | module | next | pairs
				| pcall | print | rawequal | rawget
				| rawset | require | select | setfenv
				| setmetatable | tonumber | tostring | type
				| unpack | xpcall
			) \\b
		
"""
	}

	{	# Library functions
		name: "support.function.library.lua"

		match: """
(?x)

			# Check we are *not* scoped
			(?<! [^.] . | : )
			# Look for any library name
			\\b (
				coroutine \\. (
					create | resume | running | status
					| wrap | yield )
				| debug \\. (
					debug | [gs]etfenv | [gs]ethook | getinfo
					| [gs]etlocal | [gs]etmetatable | getregistry
					| [gs]etupvalue | traceback )
				| io \\. (
					close | flush | input | lines
					| open | output | popen | read
					| tmpfile | type | white )
				| math \\. (
					abs | acos | asin | atan2?
					| ceil | cosh? | deg | exp
					| floor | fmod | frexp | ldexp
					| log | log10 | max | min
					| modf | pow | rad | random
					| randomseed | sinh? | sqrt | tanh? )
				| os \\. (
					clock | date | difftime | execute
					| exit | getenv | remove | rename
					| setlocale | time | tmpname )
				| package \\. (
					cpath | loaded | loadlib | path
					| preload | seeall )
				| string \\. (
					byte | char | dump | find
					| format | gmatch | gsub | len
					| lower | match | rep | reverse
					| sub | upper )
				| table \\. (
					concat | insert | maxn | remove
					| sort )
			) \\b
		
"""
	}

	## Standard Library Functions / Constants ##################################

	{	# Class Functions
		name: "support.function.library.lua"

		match: """
(?x)

			# Check we *are* scoped
			(?<= [^.] \\. | : )
			\\b ( SendViewModelMatchingSequence|SelectWeightedSequenceSeeded|GetCurrentWeaponProficiency|SetCurrentWeaponProficiency|SetVerticalScrollbarEnabled|AddVCDSequenceToGestureSlot|AllowImmediateDecalPainting|SetAllowNonAsciiCharacters|SetRenderClipPlaneEnabled|GetManipulateBonePosition|SetNoCollideWithTeammates|GetConstrainedPhysObjects|GetNoCollideWithTeammates|SetPaintBackgroundEnabled|GetDoubleClickingEnabled|SetAllowWeaponsInVehicle|SetPhysConstraintObjects|Weapon_TranslateActivity|InitializeAsClientEntity|SetVelocityInstantaneous|InsertClickableTextStart|GetShouldPlayPickupSound|MakePhysicsObjectAShadow|SetSuppressPickupNotices|SetDoubleClickingEnabled|GetMomentaryRotButtonPos|SetShouldPlayPickupSound|GetAllowWeaponsInVehicle|GetLocalAngularVelocity|SetAngleDragCoefficient|GWEN_SetHorizontalAlign|AnimRestartMainSequence|GetCustomCollisionCheck|GetManipulateBoneAngles|GetManipulateBoneJiggle|SetKeyBoardInputEnabled|SetDrawLanguageIDAtLeft|SetRagdollBuildFunction|EnableVerticalScrollbar|TranslateWeaponActivity|SetMinLookAheadDistance|SetKeyboardInputEnabled|SetLocalAngularVelocity|SetCustomCollisionCheck|TranslatePhysBoneToBone|GetSequenceActivityName|CalculateVelocityOffset|TranslateBoneToPhysBone|PopulateChildrenAndSelf|RagdollStopControlling|IsCarriedByLocalPlayer|ManipulateBonePosition|GetConstrainedEntities|GetShouldServerRagdoll|SelectWeightedSequence|IsKeyboardInputEnabled|SetStretchHorizontally|GetGroundSpeedVelocity|GetSequenceGroundSpeed|AddFrozenPhysicsObject|EnableCustomCollisions|FindTransitionSequence|UnfreezePhysicsObjects|GetStretchHorizontally|InsertClickableTextEnd|SetRenderInScreenshots|UseClientSideAnimation|AnimationThinkInternal|PhysicsInitMultiConvex|OptionSelectedInternal|GetAutoStretchVertical|SetShouldServerRagdoll|SetMaxRouteRebuildTime|GetCurrentAcceleration|SetAutoStretchVertical|GetManipulateBoneScale|LoadControlsFromString|DoDoubleClickInternal|SwitchToDefaultWeapon|SetPaintBorderEnabled|GetTransmitWithParent|UpdateNavButtonStatus|S