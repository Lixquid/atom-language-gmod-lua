"""
	}

	{	# Function calls
		name: "support.function.any-method.lua"

		match: """
(?x)

			# Check for a valid variable name
			\\b ( [a-zA-Z_]\\w* )
			# Check that it's called a function name
			\\b (?= \\s* ( [({"'] | \\[ =* \\[ ) )
		
"""
	}

]

repository:
	escaped_chars:
		patterns: [
			{	# Valid escapes
				name: "constant.character.escape.lua"

				match: """
(?x)

					\\\\ (
						# Decimal escape
						\\d{1,3}
					|
						# Hex escapes
						x [0-9 a-f A-F]{2}
					|
						# Escape sequences
						[abfnrtv\\\\"'\\[\\]]
					)
				
"""
			}
			{	# All other escapes are invalid
				name: "invalid.illegal.lua"

				match: "\\\\."
			}
		]
